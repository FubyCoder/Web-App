{
    "name": "raidhub",
    "version": "2023.7.15.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:proxy": "cross-env NEXTAUTH_URL=https://127.0.0.1:3000 node proxy/server.js",
        "db:start": "docker-compose -f docker-compose.yml up -d",
        "db:stop": "docker-compose -f docker-compose.yml down",
        "db:schema": "prisma migrate dev && prisma generate",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "jest",
        "destiny": "./update-manifest",
        "format": "prettier --write \"src/**/*.{ts,tsx,json}\" && prisma format",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
        "sign": "mkdir proxy/certs ; cd proxy/certs ; mkcert localhost"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^1.0.1",
        "@discordjs/collection": "^1.5.1",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.0.0",
        "bungie-net-core": "^1.1.5",
        "eslint-plugin-unused-imports": "^2.0.0",
        "next": "^13.1.6",
        "next-auth": "^4.22.1",
        "prisma": "^5.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sharp": "^0.32.2"
    },
    "devDependencies": {
        "@jest/globals": "^29.4.2",
        "@types/node": "18.0.0",
        "@types/react": "^18.0.33",
        "@types/react-dom": "18.0.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.2",
        "jest": "^29.5.0",
        "node-fetch": "^3.3.1",
        "prettier": "2.8.7",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "4.7.4"
    }
}
